          <camunda:formField id="text_congratulations" label="Congratulations" type="string" defaultValue="Congratulations! You get ${def_rewardAmount} $GURU on Mainnet" />
          <camunda:formField id="action_complete" label="Complete" type="boolean" defaultValue="false" />
