<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_GroupMessage" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.31.0" modeler:executionPlatform="Camunda Platform" modeler:executionPlatformVersion="7.20.0">
  <bpmn:process id="group_message_process" name="Group Message Processing" isExecutable="true">
    <bpmn:startEvent id="Start_GroupMessage" name="Group Message Received" camunda:initiator="camunda_user_id">
      <bpmn:extensionElements>
        <camunda:formData businessKey="">
          <camunda:formField id="form_documentId" label="KB Document ID" type="string" />
          <camunda:formField id="form_question" label="Message Text" type="string" />
          <camunda:formField id="form_chatName" label="Chat Type" type="string" defaultValue="group" />
          <camunda:formField id="config_chatID" label="Chat/Group ID" type="string" />
          <camunda:formField id="config_threadID" label="Thread ID" type="string" />
          <camunda:formField id="form_replyMessageType" label="Message Type" type="string" />
          <camunda:formField id="form_tgMessageInfo" label="Full Message Info (JSON)" type="string" />
          <camunda:formField id="config_enabledTools" label="Enabled Tools" type="string" />
          <camunda:formField id="config_knowledgeBases" label="Knowledge Bases" type="string" />
          <camunda:formField id="config_llmProvider" label="LLM Provider" type="string" />
          <camunda:formField id="config_modelName" label="Model Name" type="string" />
        </camunda:formData>
        <camunda:executionListener class="ai.hhrdr.chainflow.engine.listener.SetStartVariablesListener" event="start" />
      </bpmn:extensionElements>
      <bpmn:outgoing>Flow_ToSplit</bpmn:outgoing>
      <bpmn:messageEventDefinition messageRef="Message_2ootpvj" />
    </bpmn:startEvent>
    <bpmn:parallelGateway id="Gateway_Split">
      <bpmn:incoming>Flow_ToSplit</bpmn:incoming>
      <bpmn:outgoing>Flow_ToTracking</bpmn:outgoing>
      <bpmn:outgoing>Flow_ToProcessing</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:sequenceFlow id="Flow_ToSplit" sourceRef="Start_GroupMessage" targetRef="Gateway_Split" />
    <bpmn:userTask id="human_noreply" name="Track Message from Group" camunda:assignee="${camunda_user_id}">
      <bpmn:documentation>${execution.hasVariable('form_tgMessageInfo') &amp;&amp; !empty form_tgMessageInfo ? form_tgMessageInfo : form_question}</bpmn:documentation>
      <bpmn:extensionElements>
        <camunda:taskListener class="ai.hhrdr.chainflow.engine.listener.TaskCompleteListener" event="create" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToTracking</bpmn:incoming>
      <bpmn:outgoing>Flow_TrackingDone</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_ToTracking" sourceRef="Gateway_Split" targetRef="human_noreply" />
    <bpmn:exclusiveGateway id="Gateway_CheckEngagement" name="Check Engagement Type" default="Flow_ToParsingCheck">
      <bpmn:outgoing>Flow_DirectToQuestionDiscovery</bpmn:outgoing>
      <bpmn:outgoing>Flow_ToParsingCheck</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_ToProcessing" sourceRef="Gateway_Split" targetRef="Event_0lb6sm3" />
    <bpmn:sequenceFlow id="Flow_DirectToQuestionDiscovery" name="Reply or Mention" sourceRef="Gateway_CheckEngagement" targetRef="Task_QuestionDiscovery">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${form_replyMessageType == 'REPLY' || form_replyMessageType == 'MENTION'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:scriptTask id="Task_ParseMessage" name="Parse Message for Known Patterns" scriptFormat="JavaScript">
      <bpmn:incoming>Flow_ToParsingCheck</bpmn:incoming>
      <bpmn:outgoing>Flow_AfterParsing</bpmn:outgoing>
      <bpmn:script>// Check for x.com links or dashboard_slug
var formQuestion = execution.getVariable("form_question");
var text = (formQuestion == null || formQuestion.trim() === "") ? "" : formQuestion;

// Check for "x.com" links (case-insensitive)
if (text &amp;&amp; /x\.com/i.test(text)) {
  execution.setVariable("parsed_match_found", true);
} 
// Check for dashboard queries
else if (text &amp;&amp; /dashboard_slug/i.test(text)) {
  execution.setVariable("parsed_match_found", true);
}
// Default case
else {
  execution.setVariable("parsed_match_found", false);
}</bpmn:script>
    </bpmn:scriptTask>
    <bpmn:sequenceFlow id="Flow_ToParsingCheck" sourceRef="Gateway_CheckEngagement" targetRef="Task_ParseMessage" />
    <bpmn:exclusiveGateway id="Gateway_AfterParsing" default="Flow_ToEngagementCheck">
      <bpmn:incoming>Flow_AfterParsing</bpmn:incoming>
      <bpmn:outgoing>Flow_ParsedToQuestionDiscovery</bpmn:outgoing>
      <bpmn:outgoing>Flow_ToEngagementCheck</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_AfterParsing" sourceRef="Task_ParseMessage" targetRef="Gateway_AfterParsing" />
    <bpmn:sequenceFlow id="Flow_ParsedToQuestionDiscovery" name="Pattern Matched" sourceRef="Gateway_AfterParsing" targetRef="Task_QuestionDiscovery">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${parsed_match_found == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:serviceTask id="Task_AgentEngagement" name="Agent: Should I Engage?" camunda:type="external" camunda:topic="agent_engagement_check">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="message_text">${form_question}</camunda:inputParameter>
          <camunda:inputParameter name="kb_doc_id">${form_documentId}</camunda:inputParameter>
          <camunda:inputParameter name="chat_type">group</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToEngagementCheck</bpmn:incoming>
      <bpmn:outgoing>Flow_AfterEngagement</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToEngagementCheck" sourceRef="Gateway_AfterParsing" targetRef="Task_AgentEngagement" />
    <bpmn:exclusiveGateway id="Gateway_AfterEngagement" default="Flow_NoEngage">
      <bpmn:incoming>Flow_AfterEngagement</bpmn:incoming>
      <bpmn:outgoing>Flow_EngageToQuestionDiscovery</bpmn:outgoing>
      <bpmn:outgoing>Flow_NoEngage</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_AfterEngagement" sourceRef="Task_AgentEngagement" targetRef="Gateway_AfterEngagement" />
    <bpmn:sequenceFlow id="Flow_EngageToQuestionDiscovery" name="Should Engage" sourceRef="Gateway_AfterEngagement" targetRef="Task_QuestionDiscovery">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${should_engage == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:serviceTask id="Task_QuestionDiscovery" name="Analyze Intent" camunda:type="external" camunda:topic="question_discovery">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="message_text">${form_question}</camunda:inputParameter>
          <camunda:inputParameter name="kb_doc_id">${form_documentId}</camunda:inputParameter>
          <camunda:inputParameter name="chat_type">group</camunda:inputParameter>
          <camunda:inputParameter name="enabled_tools">${config_enabledTools}</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_DirectToQuestionDiscovery</bpmn:incoming>
      <bpmn:incoming>Flow_ParsedToQuestionDiscovery</bpmn:incoming>
      <bpmn:incoming>Flow_EngageToQuestionDiscovery</bpmn:incoming>
      <bpmn:outgoing>Flow_ToAgentRouter</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToAgentRouter" sourceRef="Task_QuestionDiscovery" targetRef="Gateway_AgentRouter" />
    <bpmn:exclusiveGateway id="Gateway_AgentRouter" name="Route to Agent" default="Flow_ToGeneralAnswer">
      <bpmn:incoming>Flow_ToAgentRouter</bpmn:incoming>
      <bpmn:outgoing>Flow_ToTwitterResearch</bpmn:outgoing>
      <bpmn:outgoing>Flow_ToDashboardAnalysis</bpmn:outgoing>
      <bpmn:outgoing>Flow_ToGeneralAnswer</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_TwitterResearch" name="Research Twitter Content" camunda:type="external" camunda:topic="twitter_research">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="message_text">${form_question}</camunda:inputParameter>
          <camunda:inputParameter name="kb_doc_id">${form_documentId}</camunda:inputParameter>
          <camunda:inputParameter name="chat_type">group</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToTwitterResearch</bpmn:incoming>
      <bpmn:outgoing>Flow_TwitterDone</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToTwitterResearch" name="Twitter Research" sourceRef="Gateway_AgentRouter" targetRef="Task_TwitterResearch">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${agent_type == 'twitter_research'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:serviceTask id="Task_DashboardAnalysis" name="Analyze Dashboard" camunda:type="external" camunda:topic="dashboard_analysis">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="message_text">${form_question}</camunda:inputParameter>
          <camunda:inputParameter name="kb_doc_id">${form_documentId}</camunda:inputParameter>
          <camunda:inputParameter name="chat_type">group</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToDashboardAnalysis</bpmn:incoming>
      <bpmn:outgoing>Flow_DashboardDone</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToDashboardAnalysis" name="Dashboard Analysis" sourceRef="Gateway_AgentRouter" targetRef="Task_DashboardAnalysis">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${agent_type == 'analyze_dashboard'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:serviceTask id="Task_GeneralAnswer" name="General Answer (RAG + KB)" camunda:type="external" camunda:topic="general_answer">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="message_text">${form_question}</camunda:inputParameter>
          <camunda:inputParameter name="kb_doc_id">${form_documentId}</camunda:inputParameter>
          <camunda:inputParameter name="knowledge_bases">${config_knowledgeBases}</camunda:inputParameter>
          <camunda:inputParameter name="chat_type">group</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToGeneralAnswer</bpmn:incoming>
      <bpmn:outgoing>Flow_GeneralDone</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToGeneralAnswer" name="General Answer" sourceRef="Gateway_AgentRouter" targetRef="Task_GeneralAnswer" />
    <bpmn:exclusiveGateway id="Gateway_MergeAgents">
      <bpmn:incoming>Flow_TwitterDone</bpmn:incoming>
      <bpmn:incoming>Flow_DashboardDone</bpmn:incoming>
      <bpmn:incoming>Flow_GeneralDone</bpmn:incoming>
      <bpmn:outgoing>Flow_ToSendResponse</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_TwitterDone" sourceRef="Task_TwitterResearch" targetRef="Gateway_MergeAgents" />
    <bpmn:sequenceFlow id="Flow_DashboardDone" sourceRef="Task_DashboardAnalysis" targetRef="Gateway_MergeAgents" />
    <bpmn:sequenceFlow id="Flow_GeneralDone" sourceRef="Task_GeneralAnswer" targetRef="Gateway_MergeAgents" />
    <bpmn:sendTask id="Task_SendResponse" name="Send Response to Group" camunda:class="ai.hhrdr.chainflow.engine.delegate.ChatbotMessageDelegate">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="chatbotMessage">${agent_response}</camunda:inputParameter>
          <camunda:inputParameter name="buttonLink">${null}</camunda:inputParameter>
          <camunda:inputParameter name="buttonText">${null}</camunda:inputParameter>
          <camunda:inputParameter name="directSend">${true}</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToSendResponse</bpmn:incoming>
      <bpmn:outgoing>Flow_ResponseSent</bpmn:outgoing>
    </bpmn:sendTask>
    <bpmn:sequenceFlow id="Flow_ToSendResponse" sourceRef="Gateway_MergeAgents" targetRef="Task_SendResponse" />
    <bpmn:parallelGateway id="Gateway_Converge">
      <bpmn:incoming>Flow_ResponseSent</bpmn:incoming>
      <bpmn:outgoing>Flow_ToEnd</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:sequenceFlow id="Flow_TrackingDone" sourceRef="human_noreply" targetRef="Event_0a175z7" />
    <bpmn:sequenceFlow id="Flow_ResponseSent" sourceRef="Task_SendResponse" targetRef="Gateway_Converge" />
    <bpmn:endEvent id="End_NoEngage" name="No Engagement Needed">
      <bpmn:incoming>Flow_NoEngage</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_NoEngage" name="Don&#39;t Engage" sourceRef="Gateway_AfterEngagement" targetRef="End_NoEngage" />
    <bpmn:endEvent id="End_Complete" name="Processing Complete">
      <bpmn:incoming>Flow_ToEnd</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_ToEnd" sourceRef="Gateway_Converge" targetRef="End_Complete" />
    <bpmn:endEvent id="Event_0lb6sm3">
      <bpmn:incoming>Flow_ToProcessing</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="Event_0a175z7">
      <bpmn:incoming>Flow_TrackingDone</bpmn:incoming>
    </bpmn:endEvent>
  </bpmn:process>
